{
  "name": "babelsheet2-reader",
  "version": "0.0.5",
  "description": "Rx.js observable that parses Babelsheet-formatted Google Spreadsheet",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run test && npm run build",
    "test": "ts-node ./node_modules/.bin/tape ./tests/*.test.ts | tap-arc",
    "lint": "eslint --fix src/**/*.ts"
  },
  "keywords": [
    "babelsheet",
    "translations",
    "i18n"
  ],
  "author": "The Software House",
  "license": "MIT",
  "dependencies": {
    "google-spreadsheet": "^3.2.0",
    "lodash.setwith": "^4.3.2"
  },
  "peerDependencies": {
    "rxjs": ">=6.0.0"
  },
  "devDependencies": {
    "@types/google-spreadsheet": "^3.1.5",
    "@types/lodash.setwith": "^4.3.6",
    "@types/node": "^17.0.2",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "tap-arc": "^0.1.2",
    "tape": "^5.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
